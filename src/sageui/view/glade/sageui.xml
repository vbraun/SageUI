<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog 
    name="sageui"
    library="gladepython">

  <init-function>glade_python_init</init-function>

  <glade-widget-classes>
    <glade-widget-class 
	title="Sage Commandline" 
	name="TerminalWidget"
	generic-name="terminal_widget"/>
  </glade-widget-classes>
  
  <glade-widget-classes>
    <glade-widget-class 
	title="Diff Viewer" 
	name="DiffViewerWidget"
	generic-name="diff_viewer_widget"/>
  </glade-widget-classes>
  

  <glade-widget-group 
      name="python" 
      title="Sage UI Widgets">
    <glade-widget-class-ref 
	name="TerminalWidget"/>
    <glade-widget-class-ref 
	name="DiffViewerWidget"/>
  </glade-widget-group>

</glade-catalog>
